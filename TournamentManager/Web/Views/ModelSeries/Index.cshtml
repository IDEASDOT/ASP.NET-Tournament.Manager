</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ModelSerieName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ModelSerieName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ModelSerieId }) |
