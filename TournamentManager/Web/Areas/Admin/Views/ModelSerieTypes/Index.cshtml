@model Web.Areas.Admin.ViewModels.ModelSerieTypeIndexViewModel

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink(@Resources.Common.CreateNew, "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ModelSerieTypes.FirstOrDefault().ModelSerieTypeName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model.ModelSerieTypes) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ModelSerieTypeName)
        </td>
        <td>
            @Html.ActionLink(@Resources.Common.Edit, "Edit", new { id=item.ModelSerieTypeId }) |
            @Html.ActionLink(@Resources.Common.Details, "Details", new { id=item.ModelSerieTypeId }) |
            @Html.ActionLink(@Resources.Common.Delete, "Delete", new { id=item.ModelSerieTypeId })
        </td>
    </tr>
}

</table>
